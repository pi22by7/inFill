{

    "manifest_version": 2,
    "name": "inFill",
    "version": "0.1",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/icon.svg"
    },
  
    "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["infill.js"]
    }
  ]  
}
  